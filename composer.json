{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "torres-developer/mvc",
    "description": "MVC base framework",
    "type": "library",
    "keywords": [
        "MVC",
        "framework"
    ],
    "homepage": "https://github.com/torres-developer/MVCBase",
    "readme": "./README.md",
    "time": "2022-10-26",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "torres-developer",
            "email": "torres.dev@disroot.org",
            "homepage": "https://github.com/torres-developer",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/torres-developer/MVCBase/issues",
        "source": "https://github.com/torres-developer/MVCBase/"
    },
    "require": {
        "php": ">=8.1.13",
        "torres-developer/pdo": "dev-master",
        "pds/skeleton": "^1.0",
        "psr/http-message": "1.0.x-dev",
        "torres-developer/pull": "dev-master",
        "torres-developer/http-message-impl": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "TorresDeveloper\\MVC\\": "src/"
        },
        "files": [
            "src/consts.php",
            "src/globalFunctions.php"
        ]
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/torres-developer/MVCBase"
        }
    ],
    "bin": [
        "bin/genBase"
    ]
}
